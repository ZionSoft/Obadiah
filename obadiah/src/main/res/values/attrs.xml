<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressBar">
        <attr name="maxProgress" format="integer|reference" />
        <attr name="textSize" format="dimension" />
        <attr name="textPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SectionHeader">
        <attr name="settingSectionHeaderText" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="SettingTitleDescriptionButton">
        <attr name="settingTitleDescriptionButtonTitle" format="string|reference" />
        <attr name="settingTitleDescriptionButtonDescription" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="Switch">
        <attr name="switchTitle" format="string|reference" />
    </declare-styleable>
</resources>
